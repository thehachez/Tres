<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta httpEquiv="x-ua-compatible" content="ie=edge" />
    <title>@ViewData["Title"] - tres_ws</title>
    <environment names="Development">
        <link rel="stylesheet" href="~/stylesheets/main_dev.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/stylesheets/main.css" asp-append-version="true" />
    </environment>

    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)

</head>
<body>

    @RenderBody()

    <environment names="Development">
        <script src="~/builds/dev/main.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/builds/production/main.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)

</body>
</html>
